interfaces:
  - id: GigabitEthernet0/0
    description: "LAN R1"
    ipv4_addresses: 192.168.1.1 255.255.255.0
    ipv6_link_local_addresses:
      - 'FE80::1'
    ipv6_ula_uga_addresses:
      - 'FD00:FD00:FD00:1::1/64'
dhcp:
  dhcp_pool:
    - id: "LANR1"
      network: 192.168.1.0
      netmask: 255.255.255.0
      router: 192.168.1.1
      dns: 8.8.8.8
  dhcp_excluded:
    - start: 192.168.1.1
      end: 192.168.1.100
nat:
  name: "LANS"
  inside:
    - GigabitEthernet0/0
  outside: GigabitEthernet0/1
  sources:
    - "192.168.1.0 0.0.0.255"
